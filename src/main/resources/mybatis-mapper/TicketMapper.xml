<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://hc.supconit.com/mybatis-3-mapper.dtd">
<mapper namespace="com.supconit.dao.mapper.DriverMapper">

    <update id="updateById" parameterType="com.supconit.dao.domain.TicketDo">
        UPDATE ticket SET stock=#{stock} WHERE id = #{id}
    </update>

    <select id="getTicketById" parameterType="java.lang.Long" resultType="com.supconit.dao.domain.TicketDo">
        select * from ticket where id =#{id}
    </select>

</mapper>
